---
import favIcon from '~/assets/favicons/favicon.ico';
import favIconSvg from '~/assets/favicons/favicon.svg';
import appleTouchIcon from '~/assets/favicons/apple-touch-icon.png';
---

<link rel="shortcut icon" href={`${import.meta.env.BASE_URL}favIcon`} />
<link rel="icon" type="image/svg+xml" href={`${import.meta.env.BASE_URL}favIconSvg.src`} />
<link rel="mask-icon" href={`${import.meta.env.BASE_URL}favIconSvg.src}` color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={`${import.meta.env.BASE_URL}appleTouchIcon.src`} />
